<!DOCTYPE html>
<html lang="en">

<head>
    <%- include layout/head %>
</head>

<body>
    <%- include layout/nav %>
        <div class="containner">
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            Kitchen Sink
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <div class="table-responsive">
                                <table class="table table-striped table-bordered table-hover">
                                    <thead>
                                        <tr>
                                            <th>ID</th>
                                            <th>Title</th>
                                            <th>Content</th>
                                            <th>Author</th>
                                            <th>Create_at</th>
                                            <th>Update_at</th>
                                            <th>Options</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% if(data && data.posts){%>
                                            <%data.posts.forEach(aa=>{%>
                                                <tr>
                                                    <td><%=aa.id%></td>
                                                    <td><%=aa.title%></td>
                                                    <td><%=aa.content%></td>
                                                    <td><%=aa.author%></td>
                                                    <td><%=aa.create_at%></td>
                                                    <td><%=aa.update_at%></td>
                                                    <td><a href="#" class="btn btn-success">Edit</a>
                                                        <a href="#" class="btn btn-danger">Delete</a>
                                                    </td>
                                                </tr>
                                                <%})%>
                                                    <%}%>

                                    </tbody>
                                </table>
                            </div>
                            <!-- /.table-responsive -->
                        </div>
                        <!-- /.panel-body -->
                    </div>
                </div>
            </div>
        </div>
</body>
<%- include layout/footer %>

</html>